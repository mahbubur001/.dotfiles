	email = mahbubur001@gmail.com
[core]
    editor = nvim
    excludesfile = ~/.gitignore_global
[pull]
    ff = only
[init]
	defaultBranch = main
[merge]
	conflictstyle = diff3
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f

#!/bin/bash
#Generated by JetBrains Toolbox 1.22.10970 at 2021-12-22T14:57:34.578812

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/tony/Library/Application Support/JetBrains/Toolbox/apps/Rider/ch-0/213.5744.299/Rider.app/Contents/MacOS/rider" $wait --args "${ideargs[@]}"